{
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.insertComments",
        "title": "Insert Comments Above `const` and `let` Declarations."
      },
      {
        "command": "extension.renameIdentifierInComments",
        "title": "Rename symbol in comments."
      }
    ]
  },
  "categories": [
    "Other"
  ],
  "description": "Provides support to anything related to CSS",
  "displayName": "CSS",
  "engines": {
    "vscode": "^1.5.0"
  },
  "name": "css",
  "publisher": "Luis",
  "version": "0.0.1",
  "main": "./extension",
 
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "css-tree": "^1.0.0-alpha25",
    "eslint": "^3.6.0",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
